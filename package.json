    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3"
  },
  "keywords": [
